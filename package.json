{
  "name": "donejs-chat",
  "version": "0.0.0",
  "description": "",
  "homepage": "",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "scripts": {
    "test": "testee src/test.html --browsers firefox --reporter Spec",
    "start": "can-serve --port 8080",
    "develop": "can-serve --develop --port 8080",
    "document": "documentjs",
    "build": "node build"
  },
  "main": "production.html",
  "files": [
    "src"
  ],
  "keywords": [],
  "system": {
    "main": "donejs-chat/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload"
    ],
    "npmIgnore": [
      "documentjs",
      "testee",
      "donejs-deploy",
      "generator-donejs",
      "feathers",
      "feathers-memory",
      "feathers-hooks",
      "body-parser"
    ],
    "envs": {
      "server-production": {
        "baseURL": "https://donejs-chat.divshot.io/"
      }
    }
  },
  "dependencies": {
    "bit-tabs": "^0.2.0",
    "body-parser": "^1.14.1",
    "bootstrap": "^3.3.5",
    "can": "^2.3.0-beta.1",
    "can-connect": "^0.3.0",
    "can-ssr": "^0.9.1",
    "done-autorender": "^0.4.2",
    "done-component": "^0.2.1",
    "done-css": "^1.1.10",
    "feathers": "^1.1.1",
    "feathers-hooks": "^0.5.1",
    "feathers-memory": "^0.3.4",
    "generator-donejs": "^0.3.0",
    "jquery": "^2.1.4",
    "steal": "^0.12.0-pre.9",
    "steal-socket.io": "^1.0.0"
  },
  "devDependencies": {
    "documentjs": "^0.3.1",
    "donejs-deploy": "^0.2.0",
    "funcunit": "^3.0.0",
    "steal-cordova": "^0.1.1",
    "steal-nw": "^0.1.3",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.12.0-pre.4",
    "testee": "^0.2.1"
  },
  "donejs": {
    "deploy": {
      "root": "dist",
      "services": {
        "production": {
          "type": "divshot",
          "config": {
            "name": "donejs-chat",
            "headers": {
              "/**": {
                "Access-Control-Allow-Origin": "*"
              }
            }
          }
        }
      }
    }
  },
  "window": {
    "toolbar": false,
    "width": 1060,
    "height": 625
  }
}
